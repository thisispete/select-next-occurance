{
	"name": "select-next-occurance",
	"displayName": "select-next-occurance",
	"description": "selects the next (or previous) occurance of the currently selected text within a file",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.selectNextOccurance",
				"title": "Select Next Occurance"
			},
			{
				"command": "extension.selectPreviousOccurance",
				"title": "Select Previous Occurance"
			}
		],
		"keybindings": [
			{
				"command": "extension.selectNextOccurance",
				"mac": "cmd+g",
				"key": "ctrl+g",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.selectPreviousOccurance",
				"mac": "cmd+shift+g",
				"key": "ctrl+shift+g",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
